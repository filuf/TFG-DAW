generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model products {
  is_unlimited      Boolean             @db.Bit(1)
  product_price     Float               @db.Float
  id_product        BigInt              @id @default(autoincrement())
  product_name      String              @db.VarChar(45)
}
